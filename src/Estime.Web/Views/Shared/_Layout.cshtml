@using Estime.Web.HtmlHelpers
<!DOCTYPE html>
<html class="no-js">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/themes/base/jquery-ui.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/960gs.css")" rel="stylesheet" type="text/css" />
	<link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />

	<script src="@Url.Content("~/Scripts/modernizr-2.0.6-development-only.js")" type="text/javascript"></script>
	<script src="@Url.Content("~/Scripts/jquery-1.7.1.js")" type="text/javascript"></script>
	@RenderSection("AdditionalScripts", required: false)
</head>
<body>
	<div class="container_4">
		<nav>
			<div class="grid_4 nav">
				<ul class="menu">
					<li class="special">@Html.MenuLink("Ny opgave", "New", "Task")</li>
					<li>@Html.MenuLink("Åbne", "Open", "Task")</li>
					<li>@Html.MenuLink("Lukkede", "Closed", "Task")</li>
					<li>@Html.MenuLink("Konsulenter", "List", "Consultant")</li>
					<li>@Html.MenuLink("Kunder", "List", "Client")</li>
					<li>@Html.MenuLink("Varekartotek", "Import", "Ware")</li>
					<li>@Html.MenuLink("Eksporter lukkede", "Export", "Task")</li>
					<li>@Html.MenuLink("Eksporter projekter", "ExportProjects", "Task")</li>
				</ul>
			</div>
		</nav>
	
		<section id="main">
			<div class="grid_4 new-task">
				<div style="margin: 10px;">
					@RenderBody()
				</div>
			</div>
		</section>
	</div>

</body>
</html>
